@model OrganizacijaSlavlja.Models.StavkaKonfiguracijeZurke

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section main
{
    <br />
    <br />
    <table class="table table-striped table-bordered table-hover table-condensed">
      <tr>
           <td class="nazivKolone">Naziv zurke</td>
           <td class="nazivKolone">Gost</td>
           <td class="nazivKolone">Proizvod</td>
           <td class="nazivKolone">Kolicina</td> 
      </tr>
       @for (int i = 0; i < ViewBag.SveStavke.Count; i++)
       {
           <tr>
           <td>@ViewBag.SveStavke[i].Zurka.Naziv</td>
           <td>@ViewBag.SveStavke[i].Gost.Nadimak</td>
           <td>@ViewBag.SveStavke[i].Proizvod.Naziv</td>
           <td>@ViewBag.SveStavke[i].Kolicina</td>
           </tr>
       }
      </table>
}
